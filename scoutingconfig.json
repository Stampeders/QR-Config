  "sections": [
    {
      "name": "Prematch",
      "fields": [
        {
          "title": "Scouter Initials",
          "description": "Enter the initials of the scouter.",
          "type": "text",
          "required": true,
          "code": "scouter",
          "formResetBehavior": "preserve",
          "defaultValue": ""
        },
        {
          "title": "Match Number",
          "description": "Enter the match number.",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "formResetBehavior": "increment",
          "defaultValue": 1
        },
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting.",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 19999
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot.",
          "type": "select",
          "required": true,
          "code": "Prsp",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Outpost Side",
            "R2": "Middle",
            "R3": "Depot Side"
          }
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Moved?",
          "description": "Check if the robot moved during autonomous.",
          "type": "boolean",
          "required": false,
          "code": "Mved",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Fuel missed",
          "description": "The amount of fuel missed in auto",
          "type": "counter",
          "required": false,
          "code": "FIA",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 5
        },
        {
          "title": "Climbed Tower",
          "description": "Did the robot climb?",
          "type": "select",
          "required": false,
          "code": "climbedAuto",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Auto Foul",
          "type": "counter",
          "required": false,
          "code": "auf",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "title": "Robot Fuel Missed",
          "description": "The amount of fuel missed in teleop (by the robot)",
          "type": "counter",
          "required": false,
          "code": "FIT",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Robot Lob Shots",
          "description": "The number of times a robot shoots a lob shot",
          "type": "counter",
          "required": false,
          "code": "LF",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Human Fuel Scored",
          "description": "The amount of fuel scored directly by the human player",
          "type": "counter",
          "required": false,
          "code": "HFS",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Played Defense?",
          "type": "boolean",
          "required": false,
          "code": "CFPDT",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "Climbed Tower",
          "type": "select",
          "required": true,
          "code": "climbed",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "L2": "Level 2",
            "L3": "Level 3",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Tipped/Fell Over?",
          "type": "boolean",
          "required": false,
          "code": "TFOT",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "Intake",
          "description": "How did the robot intake?",
          "type": "select",
          "required": true,
          "code": "robotIntake",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "G": "Ground",
            "O": "Outpost",
            "U": "Unsure"
          }
        },
        {
          "title": "Over/Under?",
          "type": "select",
          "required": true,
          "code": "over/under",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "O": "Over the Bump",
            "U": "Under the Trench",
            "B": "Both",
            "N": "Neither"
          }
        },
        {
          "title": "Field Layout",
          "description": "Reference diagram of the field",
          "type": "image",
          "required": false,
          "code": "fieldLayout",
          "formResetBehavior": "preserve",
          "defaultValue": "https://raw.githubusercontent.com/wpilibsuite/allwpilib/refs/heads/main/fieldImages/src/main/native/resources/edu/wpi/first/fields/2026-field.png"
        },
        {
          "title": "Comments",
          "description": "Where are they shooting from? \n Go to the depot in auto? \n Did they play defence? \n Human player accuracy? \n Robot stability? Tippy? Stable? \n Penalties? [code] \n Shots in inactive hub (count as miss) \n Getting beached? \n Tank or swerve? \n Interesting mechanisms/shapes?",
          "type": "text",
          "required": true,
          "code": "co",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 500
        }
      ]
    }
  ]
